{
  "owner": "pleo-io",
  "repo": "prop",
  "author": "pleo-bot-auto-versioning <ops+auto_versioning@pleo.io>",
  "onlyPublishWithReleaseLabel": false,
  "noDefaultLabels": true,
  "labels": [
    {
      "name": "major",
      "releaseType": "major",
      "changelogTitle": "ğŸ’¥ Major changes",
      "description": "Increment the major version when merged",
      "color": "#C5000B"
    },
    {
      "name": "minor",
      "releaseType": "minor",
      "changelogTitle": "ğŸ Features",
      "description": "Increment the minor version when merged",
      "color": "#F1A60E"
    },
    {
      "name": "patch",
      "releaseType": "patch",
      "changelogTitle": "ğŸ Fixes",
      "description": "Increment the patch version when merged",
      "color": "#870048"
    },
    {
      "name": "internal",
      "changelogTitle": "ğŸ  Internal",
      "description": "Changes only affect the internal API",
      "releaseType": "none",
      "color": "#696969",
      "default": true
    }
  ],
  "plugins": [
    [
      "omit-release-notes",
      {
        "username": [
          "pleo-bot-renovate"
        ]
      }
    ]
  ]
}
